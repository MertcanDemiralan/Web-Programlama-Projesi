@model IEnumerable<WebProje.Models.Yorum>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Yapan_Kullanici_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Yapilan_Film_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Yorum_Metni)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Yildiz)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Yapan_Kullanici_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Yapilan_Film_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Yorum_Metni)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Yildiz)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
